//- header.jade
div.wrapper__header
  header.header.cf
    div.header__logo
      a(href="#")
        div.logo Сервисный Центр 
          span Фаворит
    div.header__nav
      nav.nav
        ul.nav-list
          each val in ['о компании', 'корпоративным клиентам', 'контакты']
            a(href="#")
              li.nav-item= val
    div.header__tel
      div.tel
        div.tel__num
          div.num 8 (812) 
            span 143-33-45
        div.tel__call
          a.call(href="#") Перезвонить нам
    

